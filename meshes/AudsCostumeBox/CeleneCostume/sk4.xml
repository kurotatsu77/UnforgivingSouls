    <generic-constraint-default>
        <frameInB>
            <basis x="0" y="0" z="0" w="1"/>
            <origin x="0" y="0" z="0"/>
        </frameInB>
        <useLinearReferenceFrameA>false</useLinearReferenceFrameA>
        <linearLowerLimit x="0" y="-0.1" z="0"/>
        <linearUpperLimit x="0" y="0.1" z="0"/>
        <angularLowerLimit x="-0.02" y="-0.1" z="-0.001"/>
        <angularUpperLimit x=".5" y="0.1" z="0.001"/>
        <linearStiffness x="0" y="0" z="0"/>
        <angularStiffness x="0" y="10" z="0"/>
        <linearDamping x="0" y="0" z="0"/>
        <angularDamping x="0" y="0" z="0"/>
        <linearEquilibrium x="0" y="0" z="0"/>
        <angularEquilibrium x="0" y="0" z="0"/>
    </generic-constraint-default>

    <constraint-group>
        <generic-constraint bodyA="VSK_A2" bodyB="VSK_A1"></generic-constraint>
